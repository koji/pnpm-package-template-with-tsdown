{
  "name": "npm-template-with-tsup",
  "version": "0.0.5",
  "description": "npm package template with tsdown and typescript",
  "scripts": {
    "build": "tsdown ./src",
    "test": "vitest"
  },
  "homepage": "https://github.com/koji/npm-package-template-tsup",
  "url": "https://github.com/koji/npm-package-template-tsup/issue",
  "email": "kojikoji@skiff.com",
  "keywords": [
    "tsup",
    "npm",
    "template",
    "typescript"
  ],
  "author": "koji kanao",
  "license": "ISC",
  "devDependencies": {
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
